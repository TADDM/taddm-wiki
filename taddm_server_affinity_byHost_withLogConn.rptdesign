//else
//{
	// no IP address, use guid for computer system
	
	// if HOST_GUID is not null that means we need to use the host CS GUID of an AppServer or Service
	var primGuid = row["TARGET_HOST_GUID"];
	if (primGuid == null)
	{
		// HOST_GUID is null, so use the PRIMARY_GUID which should be of a CS
		primGuid = row["TARGET_PRIMARY_GUID"];
	}
	var guid = new Guid(primGuid);
	guidSet = reportContext.getPersistentGlobalVariable("guidSet");
	
