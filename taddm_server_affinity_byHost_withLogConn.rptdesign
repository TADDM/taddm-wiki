        if ((sourcePort &amp;&amp; excludedPorts[i].replace(/^\s+|\s+$/g, '') == sourcePort) || (targetPort &amp;&amp; excludedPorts[i].replace(/^\s+|\s+$/g, '') == targetPort)) {
